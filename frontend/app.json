{
  "expo": {
    "name": "SylvCiT",
    "slug": "SylvCiT",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "entryPoint": "./app/_layout.tsx",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.SylvCiT"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-name.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.SylvCiT"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Permettre à $(PRODUCT_NAME) d'accéder à votre position."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e033a0e1-6fae-420d-8d0a-1052933dfd1b"
      }
    }
  }
}